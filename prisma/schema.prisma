
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./trigger.db"
}

model Users {
  id_user  Int     @id @default(autoincrement())
  name     String
  email    String  @unique
  age      Int?
  password String
  CPF      String  @unique
  CEP      String  @unique
  isAdmin  Boolean @default(false)
}